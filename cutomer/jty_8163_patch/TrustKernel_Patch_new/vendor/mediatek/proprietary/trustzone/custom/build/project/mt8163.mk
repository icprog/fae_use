MTK_ATF_SUPPORT=yes
MTK_TEE_SUPPORT=yes
TRUSTONIC_TEE_SUPPORT=no
MTK_IN_HOUSE_TEE_SUPPORT=no
KEYMASTER_VERSION=3.0
KEYMASTER_RPMB=yes
ATF_MEMBASE=0x54600000

MEMSIZE      := 0x00400000 # 4MB, <= MAX_TEE_SIZE (192MB)
MEMSIZE_WV   := 0x01800000 # 24MB, <= MAX_TEE_SIZE (192MB)
MEMSIZE_PR   := 0x00600000 # 6MB, <= MAX_TEE_SIZE (192MB)
MEMSIZE_WVPR := 0x01800000 # 24MB, <= MAX_TEE_SIZE (192MB)
FBSIZE  := 0 # will be the secure pool size allocated from kernel.
BOOTSHARE := 0x40002000

SECURE_FUNC_STACK_NUM := 1 # number of stacks, max is 16
SECURE_FUNC_STACK_SIZE := 2048 # stack size in bytes
TZMEM_RELEASECM_SIZE := 0
MACH_TYPE := 8163

TRUSTKERNEL_TEE_SUPPORT=yes
MTK_MACH_TYPE=mt8163
MTK_TEE_DRAM_SIZE=0x600000
